'''
Docstring for leetcode-705
https://leetcode.com/problems/design-hashset/

Time Complexity:
add(): O(1)
remove: O(1)
Contains: O(1)

Space Complexity: O(n)
'''

class MyHashSet:

    def __init__(self):
        self.primaryBucket = 1000
        self.secondaryBucket = 1000
        self.hashset = [False for i in range(self.primaryBucket+1)]
        
# 1000000/1000=1000,0
# 100/11
    def getPrimaryHashKey(self, key):
        return key//self.primaryBucket

    def getSecondaryHashKey(self, key):
        return key % self.secondaryBucket

    def add(self, key: int) -> None:
        index1 = self.getPrimaryHashKey(key)

        if index1 == self.primaryBucket:
            self.hashset[self.primaryBucket] = True
            return

        if not self.hashset[index1]:
            self.hashset[index1] = [False for i in range(self.secondaryBucket)]
        
        index2 = self.getSecondaryHashKey(key)
        self.hashset[index1][index2] = True

    def remove(self, key: int) -> None:
        index1 = self.getPrimaryHashKey(key)
        if index1 == self.primaryBucket:
            self.hashset[index1] = False
            return

        if not self.hashset[index1]:
            return
        
        index2 = self.getSecondaryHashKey(key)
    
        if self.hashset[index1][index2]:
            self.hashset[index1][index2] = False

        return
        

    def contains(self, key: int) -> bool:
        index1 = self.getPrimaryHashKey(key)
        if index1 == self.primaryBucket:
            if self.hashset[index1]:
                return True
            else:
                return False

        index2 = self.getSecondaryHashKey(key)
        if not self.hashset[index1]:
            return False
        if not self.hashset[index1][index2]:
            return False
        
        return True
        


# Your MyHashSet object will be instantiated and called as such:
# obj = MyHashSet()
# obj.add(key)
# obj.remove(key)
# param_3 = obj.contains(key)